{
  "name": "agnostic",
  "version": "1.0.0",
  "description": "A library that allows other projects to be agnostic of particular http server implementation",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js adapters/*.js lib/*.js test/*.js",
    "test": "istanbul cover tape -- test/test-*.js | tap-spec",
    "win-test": "tape test/test-*.js | tap-spec",
    "debug": "tape test/test-*.js | tap-spec"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexindigo/agnostic.git"
  },
  "keywords": [
    "agnostic",
